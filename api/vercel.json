{
    "rewrites": [
      {
        "source": "/api1/(.*)",
        "destination": "https://vop.baidu.com/server_api/$1"
      },
      {
        "source": "/api2/(.*)",
        "destination": "https://aip.baidubce.com/oauth/2.0/token/$1"
      },
      {
        "source": "/api3/(.*)",
        "destination": "http://tsn.baidu.com/text2audio/$1"
      },
      {
        "source": "/api4/(.*)",
        "destination": "https://open.bigmodel.cn/api/paas/v3/model-api/$1"
      }
    ]
  }
  